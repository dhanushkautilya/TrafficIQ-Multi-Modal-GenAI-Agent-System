# FastAPI
ENVIRONMENT=development
LOG_LEVEL=info
API_TITLE=TrafficIQ
API_VERSION=0.1.0

# Vertex AI / GCP
USE_VERTEX=false
GCP_PROJECT=my-gcp-project
GCP_REGION=us-central1
VERTEX_ENDPOINT_ID=projects/my-project/locations/us-central1/endpoints/1234567890
VERTEX_MODEL_NAME=gemma-3n-tuned-vehicles

# Storage
ARTIFACTS_PATH=./artifacts
USE_GCS=false
GCS_BUCKET=my-trafficiq-bucket

# BOLO Service
BOLO_SERVICE_URL=http://localhost:8001
BOLO_API_KEY=demo-key-not-used-locally

# Case Management
CASE_SERVICE_URL=http://localhost:8002
CASE_API_KEY=demo-key-not-used-locally

# Logging
JSON_LOGGING=true

# Debug
DEBUG=false
